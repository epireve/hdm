# GEMINI_API_KEYS=AIzaSyDDAjJf4GbT83nomvIMMuOwQ511qCR0J4A,AIzaSyBBBBBBBBBBBBBBBBBBBBBBBBBBBBB,AIzaSyCCCCCCCCCCCCCCCCCCCCCCCCCCCCC

# Performance Configuration (optional)
MAX_WORKERS=8
BATCH_SIZE=10
RETRY_ATTEMPTS=3
TIMEOUT_SECONDS=300